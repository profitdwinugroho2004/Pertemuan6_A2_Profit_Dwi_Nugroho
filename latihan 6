A = {'a''b','c','d'}
B = {'c','d','e'}
C = {}
result_a = A.difference(B)
print ("a. A difference B =",result_a)
result_b = B.difference(A)
print ("b. B difference A =",result_b)
result_c = A.difference(C)
print ("c. A difference C =",result_c)
result_d = B.difference(C)
print ("d. B difference C =",result_d)
