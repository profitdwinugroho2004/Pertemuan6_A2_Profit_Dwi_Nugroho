A = {100,7,8}
B = {200,4,5}
C = {300,2,3}
D = {100,200,300}
result_a = A.Intersection(D)
print("a. A ∩ D =",result_a)
result_b = B.Intersection(D)
print("b. B ∩ D =",result_b)
result_c = C.Intersection(D)
print("a. C ∩ D =",result_c)
result_d = A.Intersection(A,B,C)
print("a. A ∩ B ∩ C ∩ D=",result_d)
